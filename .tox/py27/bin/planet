#!/Users/eric/pl/planet-client-python/.tox/py27/bin/python2.7
# EASY-INSTALL-ENTRY-SCRIPT: 'planet','console_scripts','planet'
__requires__ = 'planet'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('planet', 'console_scripts', 'planet')()
    )
